{
   "$jason":{
      "head":{
         "title":"vertical layout demo"
      },
      "body":{
         "style":{
            "background":"https://devdtoo.github.io/SDUI-Jsons/background.png",
            "border": "none"
         },
         "sections":[
            {
               "style": {
                  "align": "center"
               },
               "items":[
                  {
                     "type":"vertical",
                     "components":[
                        {
                           "type":"label",
                           "text":"Hello",
                           "style": {
                              "size": "30",
                              "color": "#ffffff",
                              "align": "center"
                           }
                        },
                        {
                           "type":"button",
                           "text":"START",
                           "style": {
                              "corner_radius": "30",
                              "align": "center",
                              "color": "#ffffff",
                              "border_color": "#ffffff",
                              "border_width": "2",
                              "width": "150"
                           }
                        }
                     ]
                  }
               ]
            }
         ]
      }
   }
}
